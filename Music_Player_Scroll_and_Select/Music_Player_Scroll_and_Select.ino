void set_pause(){     //Set "pauseTrue" to true in order to close conditional loop
  noTone(buttonPin);
  pauseTrue = true;
}

int checkPot_and_scrollLevel(){      //Return scroll value
  potPosition = analogRead(potPin);
  delay(10);
  scrollLevel = map(potPosition,0,1023,0,maxSong);
  if(scrollLevel == maxSong){
    scrollLevel--;
  }
  return scrollLevel;
}

bool checkSelect(){
  currentTime = millis();
  buttonPosition = digitalRead(buttonPin);
  if(buttonPosition == true && millis()>= time_since_last_press + debounceTime){
    time_since_last_press = millis();
    Serial.println(buttonPosition);
    return true;
  }
  else{
    return false;
  }
}

void play_song(int noteList[],float noteLength[]){
  if(song_last_played != scrollLevel){
    song_last_played = scrollLevel;
    noteIndex = 0;
  }
  long timeNotePlayed;
  bool shouldBreak;
  
  for (; noteIndex < 10000; noteIndex++) {
    tone(speakerPin, noteList[noteIndex]);
    timeNotePlayed = millis();
    shouldBreak = false;
    
    while (millis() - timeNotePlayed < noteLength[noteIndex]){
      delay(10);
      if(checkSelect() == true){
        shouldBreak = true;
        break;
      }
    }
    if (shouldBreak == true || noteList[noteIndex + 1] == -1){
      noTone(speakerPin);
      
      break;
    }
  }
}

void play_song_multi(int noteList1[], float noteLength1[], int noteList2[], float noteLength2[]){
  if(song_last_played != scrollLevel){
    song_last_played = scrollLevel;
    noteIndexMulti[0] = 0;
    noteIndexMulti[1] = 0;
  }
  long timeNotePlayed[] = {-10000, -10000};
  
  while (1 == 1) {
    if (millis() - timeNotePlayed[0] >= noteLength1[noteIndexMulti[0]]){
      tone(speakerPin, noteList1[noteIndexMulti[0]]);
      timeNotePlayed[0] = millis();
      noteIndexMulti[0]++;
    }
    if (millis() - timeNotePlayed[1] >= noteLength2[noteIndexMulti[1]]){
      tone(speakerPin1, noteList2[noteIndexMulti[1]]);
      timeNotePlayed[1] = millis();
      noteIndexMulti[1]++;
    }
    
    delay(10);

    
    if(checkSelect() == true || noteList1[noteIndexMulti[0] + 1] == -1){
      noTone(speakerPin);
      noTone(speakerPin1);
      break;
    }
  }
}

void screenScroll(int scrollLevel) {
    //The if statement is for button. Do not use with potentiometer.    
    lcd.clear();
    lcd.setCursor(0, 0);
    lcd.print(scrollLevel + 1);
    lcd.setCursor( (16 - songNames[scrollLevel][0].length() ) / 2 , 0);
    lcd.print(songNames[scrollLevel][0]);
    lcd.setCursor( (16 - songNames[scrollLevel][1].length() ) / 2 , 1);
    lcd.print(songNames[scrollLevel][1]);
    changePlayButton(1);
    delay(5);
    }

int changePlayButton(int state) {    //Changes play and pause button
    state = (state + 1) % 2;
    
    lcd.setCursor (15,0);
    lcd.write(byte(state));

    return state;
}



void setup() {
  Serial.begin(9600);
  Serial.println("Initializing...");
  pinMode(potPin,INPUT);
  pinMode(buttonPin, INPUT);
  pinMode(speakerPin, OUTPUT);
  lcd.begin(16, 2);
  lcd.createChar(0, pauseButton);
  lcd.createChar(1, playButton);
  screenScroll(checkPot_and_scrollLevel());
  
}

void loop() {
  while(checkSelect() == false){
    screenScroll(checkPot_and_scrollLevel());
  }
  Serial.println("Button pressed!");
  if (scrollLevel == 0){
    Serial.println("Playing Poker Face...");
    play_song(POKER_FACE_NOTE_LIST,POKER_FACE_NOTE_LENGTHS);
  }
  if(scrollLevel == 1){
    play_song(IMPERIAL_MARCH_NOTE_LIST,IMPERIAL_MARCH_NOTE_LENGTHS);
  }
  if(scrollLevel == 2){
    play_song(SUPER_MARIO_BROS_NOTE_LIST,SUPER_MARIO_BROS_NOTE_LENGTHS);
  }

  if (scrollLevel == 3){
    //play_song_multi(THROUGH_THE_FIRE_AND_FLAMES_NOTE_LIST_1, THROUGH_THE_FIRE_AND_FLAMES_NOTE_LENGTHS_1, THROUGH_THE_FIRE_AND_FLAMES_NOTE_LIST_2, THROUGH_THE_FIRE_AND_FLAMES_NOTE_LENGTHS_2);
    play_song(THROUGH_THE_FIRE_AND_FLAMES_NOTE_LIST_1, THROUGH_THE_FIRE_AND_FLAMES_NOTE_LENGTHS);
  }

  if (scrollLevel == 4){
    play_song(NEVER_GONNA_GIVE_YOU_UP_NOTE_LIST_1, NEVER_GONNA_GIVE_YOU_UP_NOTE_DURATIONS_1);
    //play_song_multi(NEVER_GONNA_GIVE_YOU_UP_NOTE_LIST_1, NEVER_GONNA_GIVE_YOU_UP_NOTE_DURATIONS_1, NEVER_GONNA_GIVE_YOU_UP_NOTE_LIST_2, NEVER_GONNA_GIVE_YOU_UP_NOTE_DURATIONS_2);
  }
}


